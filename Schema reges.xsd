            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="SporuriSalariu" nillable="true" type="reges:ArrayOfSpor">
            <xs:annotation>
              <xs:documentation>Lista de sporuri salariale</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" name="StareCurenta" type="reges:Actiune">
