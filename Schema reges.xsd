      <xs:documentation>Mesaj trimis pentru procesarea unei propuneri de detasare contract</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="reges:Message">
        <xs:sequence>
          <xs:sequence>
