            <xs:annotation>
              <xs:documentation>Flag ce marcheaza contractul ca apartinand unui angajator care isi desfasoara activitatea in conformitate cu legea 153/2017</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" nillable="true" name="DetaliiL153" type="reges:DetaliiL153">
            <xs:annotation>
              <xs:documentation>Detalii specifice legii 153/2017</xs:documentation>
