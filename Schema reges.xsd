              <xs:documentation>Campul a fost mutat la nivelul salariatului. Nu se mai transmite de catre client</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" nillable="true" name="GradInvaliditate" type="reges:GradInvaliditate">
